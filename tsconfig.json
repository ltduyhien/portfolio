{
  "compilerOptions": {
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "strictBindCallApply": true,
    "strictPropertyInitialization": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "lib": ["dom", "esnext"],
    "module": "esnext",
    "types": ["vite/client", "react", "react-dom", "node"],
    "moduleResolution": "node"
  }
} 